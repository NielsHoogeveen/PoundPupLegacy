    <LabeledNullableLinkViewer Value="Model.CountryTo" Label="To" />
</CaseViewer>
@code {
    [Parameter]
    [EditorRequired]
    public override DeportationCase Model { get; set; } = default!;

    protected override string ItemDescription => "deportation case";
}
