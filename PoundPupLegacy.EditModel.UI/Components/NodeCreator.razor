@typeparam TEditModel where TEditModel : class, Node
@inherits ViewerBase
@inject IViewModelRetrieveService<TEditModel> editService

@code {

    protected TEditModel? Model = default!;

    protected override async Task OnInitializedAsync()
    {
        Model = await editService.GetViewModelAsync(userId: User.Id, tenantId: Tenant.Id);
        StateHasChanged();
    }

    public string Title => $"Create {Model?.NodeDetails.NodeTypeName}";
}
