@using PoundPupLegacy.Common;
@if(Value is not null && Label is not null)
{
    <div>
        <label>@Label: </label>
        @Value.ToDisplayString()
    </div>
}
@code {
    [Parameter]
    [EditorRequired]
    public FuzzyDate? Value { get; set; }

    [Parameter]
    public string? Label { get; set; }
}
