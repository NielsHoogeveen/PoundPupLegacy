@using PoundPupLegacy.ViewModel.Models
@if (Links is not null && Links.Any())
{
    <h4>@Label</h4>
    <LinkListViewer Links="@Links" />
}
@code{
    [Parameter]
    [EditorRequired]
    public required IEnumerable<Link> Links { get; init; }

    [Parameter]
    [EditorRequired]
    public required string Label { get; init; }
}