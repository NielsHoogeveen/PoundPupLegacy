@using PoundPupLegacy.ViewModel.Models
@if (Links is not null && Links.Any())
{
    <ul>
        @foreach (var link in Links)
        {
            <LinkListElementViewer Link="@link" />
        }
    </ul>
}
@code{
    [Parameter]
    [EditorRequired]
    public IEnumerable<Link>? Links { get; set; }
}