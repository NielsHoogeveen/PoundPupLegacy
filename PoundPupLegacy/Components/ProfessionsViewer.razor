@using PoundPupLegacy.ViewModel.Models
@if (Model is not null && Model.Any())
{
    <h3>Professions</h3>
    <div class="person-types">
        @foreach (var link in Model)
        {
            <div><a href="@link.Path">@link.Title</a></div>
        }
    </div>
}
@code{
    [Parameter]
    [EditorRequired]
    public IEnumerable<BasicLink>? Model { get; set; }
}