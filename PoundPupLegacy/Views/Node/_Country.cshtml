@using PoundPupLegacy.ViewModel;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model Country

@foreach(var organizationType in Model.OrganizationTypes)
{
    <div>
        @organizationType.OrganizationTypeName
    </div>
    <ul>
    @foreach(var organization in organizationType.Organizations)
    {
        <li>
            <a href="@organization.Path">@organization.Name</a>
        </li>
    }
    </ul>
}

<partial name="_Documents" model="@Model.Documents" />
<partial name="_AdoptionImports" model="@Model.AdoptionImports" />


@{
}
