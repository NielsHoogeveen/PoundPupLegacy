@using PoundPupLegacy.ViewModel;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model Person



<div class="text">
    @Html.Raw(Model.Description.Replace(@"href=""http://poundpuplegacy.org", @"href="""))
</div>
<div>
    @Model.FirstName
</div>
<div>
    @Model.MiddleName
</div>
<div>
    @Model.LastName
</div>
<div>
    @Model.FullName
</div>
<div>
    @Model.Suffix
</div>
<div>
    @Model.DateOfBirth
</div>
<div>
    @Model.DateOfDeath
</div>
<partial name="_Professions" model="@Model.Professions" />
<partial name="_PartyCaseType" model="@Model.PartyCaseTypes" />
<partial name="_OrganizationPersonRelations" model="@Model.OrganizationPersonRelations" />
<partial name="_InterPersonalRelations" model="@Model.InterPersonalRelations" />
<partial name="_PartyPoliticalEntityRelations" model="@Model.PartyPoliticalEntityRelations" />
<partial name="_Documents" model="@Model.Documents" />
<partial name="_Locations" model="@Model.Locations" />

