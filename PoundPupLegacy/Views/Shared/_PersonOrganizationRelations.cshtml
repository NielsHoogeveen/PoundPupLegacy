@using PoundPupLegacy.ViewModel;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<PersonOrganizationRelation>
@if (Model.Any())
{
    <h3>Persons</h3>
    <table class="person-organization-relations" style="width:100%;">
        @foreach (var relation in Model)
        {
            <tr>
                <td><a href="@relation.Person.Path">@relation.Person.Name</a></td>
                <td>@relation.RelationTypeName</td>
                <td>@relation.DateFrom</td>
                <td>@relation.DateTo</td>
            </tr>
        }
    </table>
}
