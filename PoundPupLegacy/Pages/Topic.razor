@page "/topic/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.BasicNameable>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <BasicNameableViewer Model="@Model" />
        </Viewer>
        <Editor>
           <BasicNameableUpdater NodeId="@Id"/>
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
