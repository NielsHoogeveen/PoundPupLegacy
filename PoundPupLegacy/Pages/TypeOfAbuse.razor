@page "/type_of_abuse/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.TypeOfAbuse>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <TypeOfAbuseViewer Model="@Model" />
        </Viewer>
        <Editor>
            <TypeOfAbuseUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
