@page "/wrongful_removal_case/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.WrongfulRemovalCase>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <WrongfulRemovalCaseViewer Model="@Model" />
        </Viewer>
        <Editor>
           <WrongfulRemovalCaseUpdater NodeId="@Id"/>
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
