@page "/fathers_rights_violation_case/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.FathersRightsViolationCase>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <FathersRightsViolationCaseViewer Model="@Model" />
        </Viewer>
        <Editor>
           <FathersRightsViolationCaseUpdater NodeId="@Id"/>
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
