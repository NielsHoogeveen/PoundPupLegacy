@page "/denomination/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.Denomination>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <DenominationViewer Model="@Model" />
        </Viewer>
        <Editor>
            <DenominationUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
