@page "/organization_type/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.OrganizationType>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <OrganizationTypeViewer Model="@Model" />
        </Viewer>
        <Editor>
            <OrganizationTypeUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
