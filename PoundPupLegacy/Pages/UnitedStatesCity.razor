@page "/united_states_city/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.UnitedStatesCity>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <UnitedStatesCityViewer Model="@Model" />
        </Viewer>
        <Editor>
            <UnitedStatesCityUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
