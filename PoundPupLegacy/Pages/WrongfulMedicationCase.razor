@page "/wrongful_medication_case/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.WrongfulMedicationCase>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <WrongfulMedicationCaseViewer Model="@Model" />
        </Viewer>
        <Editor>
           <WrongfulMedicationCaseUpdater NodeId="@Id"/>
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
