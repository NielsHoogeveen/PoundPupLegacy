@page "/united_states_political_party/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.UnitedStatesPoliticalParty>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <UnitedStatesPoliticalPartyViewer Model="@Model" />
        </Viewer>
        <Editor>
            <OrganizationUpdater NodeId="Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
