@page "/profession/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.Profession>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <ProfessionViewer Model="@Model" />
        </Viewer>
        <Editor>
            <ProfessionUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
