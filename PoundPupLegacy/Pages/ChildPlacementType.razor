@page "/child_placement_type/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.ChildPlacementType>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <ChildPlacementTypeViewer Model="@Model" />
        </Viewer>
        <Editor>
            <ChildPlacementTypeUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
