@page "/type_of_abuser/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.TypeOfAbuser>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <TypeOfAbuserViewer Model="@Model" />
        </Viewer>
        <Editor>
            <TypeOfAbuserUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
