@page "/document_type/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.DocumentType>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <DocumentTypeViewer Model="@Model" />
        </Viewer>
        <Editor>
            <DocumentTypeUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
