@page "/united_states_state/{Id:int}"
@using PoundPupLegacy.Components
@inherits NodeViewerPage<ViewModel.Models.UnitedStatesState>
<NodePageViewer Model="Model">
    <NodeHandler Model="@Model">
        <Viewer>
            <UnitedStatesStateViewer Model="@Model" />
        </Viewer>
        <Editor>
            <BasicNameableUpdater NodeId="@Id" />
        </Editor>
    </NodeHandler>
</NodePageViewer>
@code {
    [Parameter]
    public override int Id { get; set; }

}
